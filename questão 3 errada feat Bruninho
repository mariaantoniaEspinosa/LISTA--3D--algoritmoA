//questão 3 da lista 3D - MARIA ANTÔNIA ESPINOSA
#include <stdio.h>
int main (){
	int cel, fah;
	
	printf ("Converso de Fahrenheit para Celsius (50 graus - 70 graus)\n");
	
	for (fah=50;fah<=70;fah++){
		cel = (fah-32)/1.8;
		printf ("\n%d graus Fahrenheit = %d graus celsius", fah, cel);
	}
	return 0;
}
